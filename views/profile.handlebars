<body>
  <main>
    <main class="profile-image">
      <div class="profile-row">
        <div class="profile-details-box">
          <h3>Age:</h3>
          <p>{{p.age}}</p>
        </div>
        <div class="profile-details-box">
          <h3>Pumps Per Week:</h3>
          <p>{{p.freq}}</p>
        </div>
      </div>
      <div class="profile-row">
        <div class="profile-details-box">
          <h3>About Me:</h3>
          <p>{{p.about}}</p>
        </div>
        <div class="profile-details-box">
          <h3>I'm Looking For:</h3>
          <p>{{p.search}}</p>
        </div>
      </div>
      <div class="profile-details-box mx-auto">
        <h3>My Go To Work Out Song:</h3>
        <p>{{p.song}}</p>
      </div>

